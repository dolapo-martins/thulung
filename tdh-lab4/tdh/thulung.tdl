; Type assigning empty mod list. Added to basic types for nouns, verbs and determiners.

non-mod-lex-item := lex-item &
  [ SYNSEM.LOCAL.CAT.HEAD.MOD < > ].

;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Matrix Type Addenda
;;;;;;;;;;;;;;;;;;;;;;;;;

basic-head-comp-phrase :+ [ SYNSEM [ LIGHT -,
             LOCAL.CAT.MC #mc ],
    HEAD-DTR.SYNSEM.LOCAL.CAT.MC #mc ].

head :+ [ PRON bool,
    AUX bool,
    POSSESSOR poss,
    FORM form ].

+np :+ [ CASE-MARKED bool,
    CASE case ].

inflected :+ [ PAST-AGENT-ROLE-MARKER-TRANSITIVE-FLAG luk,
    TENSE-MARKING-POSITION-CLASS-FLAG luk,
    1SG-PATIENT-PAST-FLAG luk,
    VERB5-TRANSITIVE-VERB-FLAG luk,
    TRANSITIVE-VERB-FLAG luk,
    1DI-PATIENT-PAST-FLAG luk,
    1PE-PATIENT-PAST-FLAG luk,
    1DE-PATIENT-PAST-FLAG luk,
    PAST-PATIENT-ROLE-MARKER-FLAG luk,
    VERB58-INTRANSITIVE-VERB-FLAG luk,
    NON-PAST-TENSE-FLAG luk,
    POSSESSIVE-FLAG luk,
    PAST-TENSE-FLAG luk,
    INTRANSITIVE-VERB-FLAG luk,
    VERB5-TRANSITIVE-VERB-OR-VERB58-INTRANSITIVE-VERB-FLAG luk,
    1PI-PATIENT-PAST-FLAG luk,
    ASPECT-MARKING-POSITION-CLASS-FLAG luk ].

infl-satisfied :+ [ PAST-AGENT-ROLE-MARKER-TRANSITIVE-FLAG na-or-+,
    TENSE-MARKING-POSITION-CLASS-FLAG na-or-+,
    1SG-PATIENT-PAST-FLAG na-or-+,
    VERB5-TRANSITIVE-VERB-FLAG na-or-+,
    TRANSITIVE-VERB-FLAG na-or-+,
    1DI-PATIENT-PAST-FLAG na-or-+,
    1PE-PATIENT-PAST-FLAG na-or-+,
    1DE-PATIENT-PAST-FLAG na-or-+,
    PAST-PATIENT-ROLE-MARKER-FLAG na-or-+,
    VERB58-INTRANSITIVE-VERB-FLAG na-or-+,
    NON-PAST-TENSE-FLAG na-or-+,
    POSSESSIVE-FLAG na-or-+,
    PAST-TENSE-FLAG na-or-+,
    INTRANSITIVE-VERB-FLAG na-or-+,
    VERB5-TRANSITIVE-VERB-OR-VERB58-INTRANSITIVE-VERB-FLAG na-or-+,
    1PI-PATIENT-PAST-FLAG na-or-+,
    ASPECT-MARKING-POSITION-CLASS-FLAG na-or-+ ].

png :+ [ PERNUM pernum ].

cat :+ [ POSSESSUM poss ].

poss := *top* &
  [ POSS-AGR png ].

basic-bare-np-phrase :+ [ SYNSEM.LOCAL.CAT [ VAL.SPEC < >,
                       HEAD #head,
                       POSSESSUM #possessum ],
    HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD #head,
                                POSSESSUM #possessum & nonpossessive ] ].

basic-head-1st-comp-phrase :+ [ SYNSEM.LOCAL.CAT.POSSESSUM #poss,
    HEAD-DTR.SYNSEM.LOCAL.CAT.POSSESSUM #poss ].

basic-head-opt-subj-phrase :+ [ HEAD-DTR.SYNSEM.LOCAL.CAT.VAL.COMPS < > ].

top-coord-rule :+ [ SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                       POSSESSUM #possessum ],
    RCOORD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                                  POSSESSUM #possessum ],
    LCOORD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                                  POSSESSUM #possessum ] ].

bottom-coord-phrase :+ [ SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                       POSSESSUM #possessum ],
    NONCONJ-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                                   POSSESSUM #possessum ] ].

;;;;;;;;;;;;;;
;;; Features
;;;;;;;;;;;;;;

;;; Pernum

pernum := *top*.
du := pernum.
2nd := pernum.
1du := 1st & du.
1du_incl := 1du & 1du_incl+2du+3du.
1du_excl := 1du.
2du := 2nd & 2du+3du.
1pl := 1st & pl.
1pl_incl := 1pl.
1pl_excl := 1pl.
2pl := 2nd & pl & 2sg+3sg+2pl+2du+3du.
3pl := 3rd & pl & 2sg+3sg+2pl+2du+3du+3pl.
1sg := 1st & sg.
2sg := 2nd & 2sg+plain & polite+2sg & 2sg+3sg.
1polite := 1st & polite.
2polite := 2nd & polite & 2polite+pl.
3polite := 3rd & polite & pl+3polite.
1plain := 1st & plain.
2plain := 2nd & plain.
3plain := 3rd & plain.
2sg+plain := pernum.
plain := 2sg+plain & 3sg+plain.
polite+2sg := pernum.
polite := polite+2sg & polite+3sg.
3sg+plain := pernum.
3sg := 3sg+plain & polite+3sg & 2sg+3sg & 3sg+3du.
1st+sg := pernum.
1st := 1st+sg.
sg := 1st+sg.
polite+3sg := pernum.
3rd+pl := pernum.
3rd := 3rd+pl.
2polite+pl := pernum.
pl := 2polite+pl & pl+3polite.
pl+3polite := 3rd+pl.
2sg+3sg := sg & 2sg+3sg+2pl+2du+3du.
3du := 2du+3du & 3sg+3du.
2sg+3sg+2pl+2du+3du+3pl := pernum.
2sg+3sg+2pl+2du+3du := 2sg+3sg+2pl+2du+3du+3pl.
2du+3du := 2sg+3sg+2pl+2du+3du & 1du_incl+2du+3du.
3sg+3du := 2sg+3sg+2pl+2du+3du & 3rd.
1du_incl+2du+3du := du.

;;; Possession

possession := *top*.
possessive := poss.  ; supertype for possessive features
nonpossessive := poss.  ; type for nonpossessive
possessor := possessive.
possessum := possessive.
possessive-pron-1 := possessive.  ; supertype for strategy pron-1
possessor-pron-1 := possessor & possessive-pron-1.
possessum-pron-1 := possessum & possessive-pron-1.

;;; Case

case := *top*.
real-case := case.  ; intermediate case type for all real cases
NOM := real-case.  ; NOM
DAT := real-case.  ; DAT
ABL := real-case.  ; ABL
GEN := real-case.  ; GEN
COM := real-case.  ; COM
lev := real-case.  ; levLOC
hiL := real-case.  ; hiLOC
loL := real-case.  ; loLOC
LOC := real-case.  ; LOC
ERG+INS := real-case.
ERG := ERG+INS.
INS := ERG+INS.
non-real-case := real-case.

;;; Form

form := *top*.
nonfinite := form.
finite := form.

;;; Tense

nonpast := tense.
past := tense.

;;; Aspect

POT := aspect.
HAB := aspect.
DEF := aspect.
STA := aspect.
PON := aspect.
RES := aspect.
ITF := aspect.
APX := aspect.

;;; Mood

IRR := mood.
IMP := mood.
OBL := mood.

